{
  "name": "moc-solucoes/moc-uteis",
  "type": "library",
  "description": "Uteis library to developer.",
  "keywords": [
    "money",
    "SlackNotification",
    "slack",
    "Notification",
    "E-mail",
    "Functions",
    "Password",
    "Trasaction"
  ],
  "homepage": "https://github.com/maikees/moc-uteis",
  "license": "MIT",
  "authors": [
    {
      "name": "MOC Soluções",
      "email": "contato@mocsolucoes.com.br",
      "homepage": "http://mocsolucoes.com.br",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "guzzlehttp/guzzle": ">=6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "phpunit/phpunit-mock-objects": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "MOCUtils\\Helpers\\": "lib/",
      "MOCUtils\\Helpers\\Commands\\": "commands/"
    },
    "files": [
      "lib/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MocOrm\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "phpunit"
    ],
    "test-cover": "phpunit --coverage-text"
  }
}
